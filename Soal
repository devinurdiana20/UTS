package soal1;
public class Vehicle {
    private String maker;
    private int kmperGallon;
    
    void Vehicle(String maker){
        
    }
    void Vehicle(int kmperGallon){
    
    }
    public void setmaker(String maker){
        this.maker = maker;
    }
    public String getmaker(){
        return this.maker;
    }  
    
    public void setkmperGallon(int kmperGallon){
        this.kmperGallon = kmperGallon;
    }
    public int getkmperGallon(){
        return this.kmperGallon;
    }  
}
